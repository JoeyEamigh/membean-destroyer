{
  "manifest_version": 3,
  "name": "Membean Destroyer",
  "description": "A chrome extension to destroy Membean",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://membean.com/*"
      ],
      "js": [
        "contentScript.bundle.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "content.styles.css",
        "icon-128.png",
        "icon-34.png"
      ],
      "matches": [
        "https://membean.com/*"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "storage"
  ]
}